<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="launcherFragmentLogo">
        <item name="android:layout_width">@dimen/dp200</item>
        <item name="android:layout_height">@dimen/dp200</item>
        <item name="android:src">@drawable/splash_logo</item>
    </style>

    <style name="listBrowserImage">
        <item name="android:layout_width">@dimen/dp120</item>
        <item name="android:layout_height">@dimen/dp120</item>
        <item name="android:scaleType">fitCenter</item>
    </style>

    <style name="listBrowserLabelLeft">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/dp20</item>
    </style>

    <style name="listBrowserLabelRight">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/dp10</item>
    </style>

    <style name="listBrowserMobileIcon">
        <item name="android:layout_width">@dimen/dp25</item>
        <item name="android:layout_height">@dimen/dp25</item>
        <item name="android:src">@drawable/mobile</item>
    </style>

    <style name="listBrowserWebIcon">
        <item name="android:layout_width">@dimen/dp25</item>
        <item name="android:layout_height">@dimen/dp25</item>
        <item name="android:src">@drawable/web</item>
        <item name="android:layout_marginTop">@dimen/dp30</item>
    </style>

    <style name="listBrowserItemGuidelineVertical">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="listBrowserItemGuidelineHorizontal">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="navigationFragmentContainer">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:name">androidx.navigation.fragment.NavHostFragment</item>
    </style>

    <style name="browserViewerFragmentAppBar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="browserViewerFragmentToolbarConstraint">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="browserViewerFragmentBackBtn">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@android:drawable/ic_menu_revert</item>
        <item name="android:layout_marginTop">@dimen/dp16</item>
        <item name="android:background">?attr/selectableItemBackgroundBorderless</item>
    </style>

    <style name="browserViewerFragmentLabelTittle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginStart">@dimen/dp16</item>
        <item name="android:layout_marginTop">@dimen/dp16</item>
        <item name="android:text">@string/webBrowserViewerFragmentToolBarTittle</item>
        <item name="android:textAppearance">@style/TextAppearance.MaterialComponents.Headline6</item>
    </style>

    <style name="browserViewerFragmentWebView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp0</item>
    </style>

    <style name="browserFragmentAppBar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="browserFragmentChipGroup">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="browserFragmentList">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp0</item>
    </style>


</resources>